  "scripts": {
    "inst": "npm install -f",
    "build": "nx build",
    "build:all": "npm run _update-build-version & nx run-many --all --target=build  && npm run concat-css && npm run concat-css-2",
    "build:table-website": "nx run table-website:build:production",
    "build:demo-table-models": "nx run demo-table-models:build:production",
    "build:preact-table": "nx run @guiexpert/preact-table:build:production",
    "build:solid-table": "nx run @guiexpert/solid-table:build:production",
    "build:table": "nx run @guiexpert/table:build:production && npm run concat-css && npm run concat-css-2",
    "concat-css": "cat libs/table/css/table*.css > dist/libs/table/css/main.css",
    "concat-css-2": "cat libs/table/css/table*.css > libs/table/css/main.css",
    "build:angular-table": "npm run _update-build-version & nx run @guiexpert/angular-table:build:production",
    "build:react-table": "nx run @guiexpert/react-table:build:production",
    "build:vue3-table": "nx run @guiexpert/vue3-table:build:production",
    "build:webcomponent-table": "nx run @guiexpert/webcomponent-table:build:production",
    "_update-build-version": "node replace.build.version.js",
    "ng": "nx",
    "nx": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "start:table-website": "nx run table-website:serve:development",
    "start:angular-table-demo": "nx run angular-table-demo:serve:development",
    "start:async-server-demo": "nx run async-server-demo:serve",
    "start:webcomponent-table-demo": "nx run webcomponent-table-demo:serve:development",
    "start:svelte-table-demo": "nx run svelte-table-demo:serve:development",
    "start:solid-table-demo": "nx run solid-table-demo:serve:development",
    "start:react-table-demo": "nx run react-table-demo:serve:development",
    "start:preact-table-demo": "nx run preact-table-demo:serve:development",
    "start:html5-table-demo": "nx run html5-table-demo:serve:development",
    "start:vue3-table-demo": "nx run vue3-table-demo:serve:development",
    "test": "nx test",
    "test:table-website": "nx run table-website:test",
    "e2e": "nx e2e --verbose",
    "e2e:table-website": "nx e2e table-website-e2e",
    "e2e:angular-table-demo": "nx e2e angular-table-demo-e2e",
    "e2e:webcomponent-table-demo": "nx e2e webcomponent-table-demo-e2e",
    "e2e:svelte-table-demo": "nx e2e svelte-table-demo-e2e",
    "e2e:solid-table-demo": "nx e2e solid-table-demo-e2e",
    "e2e:react-table-demo": "nx e2e react-table-demo-e2e",
    "e2e:preact-table-demo": "nx e2e preact-table-demo-e2e",
    "e2e:html5-table-demo": "nx e2e html5-table-demo-e2e",
    "az:login": "az login",
    "az:login-guiexpertregistry": "az acr login --name guiexpertregistry",
    "az:build:dist": "npm run build:all",
    "az:build:dockerx": "docker buildx build --platform linux/amd64 --tag guiexpert:latest --file  deploy/web/Dockerfile .",
    "az:tag": "docker tag guiexpert guiexpertregistry.azurecr.io/guiexpert/web",
    "az:push": "docker push guiexpertregistry.azurecr.io/guiexpert/web",
    "crt2cer": "openssl x509 -inform pem -in gui_expert.crt -outform der -out gui_expert.cer",
    "do:buildx": "docker buildx build --platform linux/amd64 --tag guiexpert:latest --file  deploy/web/Dockerfile .",
    "do:login": "doctl registry login",
    "do:tag": "docker tag guiexpert registry.digitalocean.com/guiexpert/guiexpert:latest",
    "do:push": "docker push registry.digitalocean.com/guiexpert/guiexpert:latest",
    "docker:images": "docker images",
    "docker:logs:guiexpert": "docker logs $(docker ps | grep  ' guiexpert' | awk '{ print $1 }') ",
    "docker:logs-f:guiexpert": "docker logs $(docker ps | grep  ' guiexpert' | awk '{ print $1 }') -f",
    "docker:container-ls": "docker container ls",
    "docker:image-remove": "docker image rm guiexpert_web -f",
    "docker:container-remove": "docker container rm guiexpert -f",
    "docker:compose:build": "docker compose build",
    "docker:compose:up": "docker compose up",
    "docker:compose:up-and-build": "docker compose up --build",
    "npm-pub:table": "npm publish dist/libs/table --access public",
    "npm-pub:angular-table": "npm publish dist/libs/angular-table --access public",
    "npm-pub:demo-table-models": "npm publish dist/libs/demo-table-models --access public",
    "npm-pub:preact-table": "npm publish dist/libs/preact-table --access public",
    "npm-pub:react-table": "npm publish dist/libs/react-table --access public",
    "npm-pub:vue3-table": "npm publish dist/libs/vue3-table --access public",
    "npm-pub:solid-table": "npm publish dist/libs/solid-table --access public",
    "npm-pub:webcomponent-table": "npm publish dist/libs/webcomponent-table --access public"


